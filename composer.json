{
    "name": "maartendeblock/yuki-api-client",
    "description": "A PHP client for interacting with the Yuki API",
    "type": "library",
    "require": {
        "phpro/soap-client": "^3.3",
        "symfony/http-client": "^6.4",
        "nyholm/psr7": "^1.8"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MaartenDeBlock\\YukiApiClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Maarten De Block",
            "email": "info@maartendeblock.be"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
